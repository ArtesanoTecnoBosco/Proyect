										<GradientStop Color="#BFF5ABAB" Offset="0.432"/>
										<GradientStop Color="#BFC6E0DD" Offset="0.638"/>
										<GradientStop Color="#BFF5ABAB" Offset="0.822"/>
									</LinearGradientBrush>
								</Rectangle.Fill>
							</Rectangle>
						</Grid>
						<ControlTemplate.Triggers>
							<Trigger Property="IsFocused" Value="True"/>
